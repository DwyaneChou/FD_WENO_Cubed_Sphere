&time_settings
run_days         = 50
run_hours        = 0
run_minutes      = 0
run_seconds      = 0
dt               = 200
history_interval = 3600
integral_scheme  = 'RK4' ! Choose from 'RK3_TVD' or 'RK4'
/

&case_select
case_num    = 5, ! Choose from 2, 5 or 6
/

&domain
mesh_file   = 'ccs_output_1p0_2nd.nc'
xhalo       = 4,  ! The ghost cell size
yhalo       = 4,  ! The ghost cell size
/

&dynamic_opt
reconstruct_scheme = 'WENO'
/
